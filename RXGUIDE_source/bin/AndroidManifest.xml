<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rxguide"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="16" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/rx_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.rxguide.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.rxguide.Doctor"
            android:label="@string/title_activity_doctor"
            android:parentActivityName="com.example.rxguide.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.rxguide.MainActivity" />
        </activity>
        <activity
            android:name="com.example.rxguide.Clinic_Admin"
            android:label="@string/title_activity_clinic__admin"
            android:parentActivityName="com.example.rxguide.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.rxguide.MainActivity" />
        </activity>
        <activity
            android:name="com.example.rxguide.Doctordetails"
            android:label="@string/title_activity_doctordetails"
            android:parentActivityName="com.example.rxguide.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.rxguide.MainActivity" />
        </activity>
        <activity
            android:name="com.example.rxguide.Prescriptiondisplay"
            android:label="@string/title_activity_prescriptiondisplay" >
        </activity>
        <activity
            android:name="com.example.rxguide.PatientID"
            android:label="@string/title_activity_patient_id" >
        </activity>
        <activity
            android:name="com.example.rxguide.PrintView"
            android:label="@string/title_activity_print_view" >
        </activity>
        <activity
            android:name=".PrintDialogActivity" >
        </activity>

    </application>

</manifest>
